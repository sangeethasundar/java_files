import java.util.ArrayList;

class Main {
  public static void main(String[] args) {
    int n=7,k=0;
    ArrayList<String> al=new ArrayList<String>();
    String s;
    for(int i=1;i<=n;i++){
      for(int j=1;j<=n;j++){
        if(k==0){
          System.out.print("1");
          k=1;
        }
        else if(k==1){
          System.out.print("0");
          k=0;
        }
      }
      System.out.println();  
    }
  }
}

output:
1010101
0101010
1010101
0101010
1010101
0101010
1010101
